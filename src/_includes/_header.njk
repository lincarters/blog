<header class="header">
  <h3 class="header__title">
    <a href="/">{{ site.title }}</a>
{% set navPages = collections.all | eleventyNavigationBreadcrumb(eleventyNavigation.key,{ includeSelf: false }) %}


<nav class="nav__breadcrumb">
<ul>
{%- for item in navPages %}
  <li>{% if item.url == page.url %}{{ item.title }}{% else %}<a href="{{ item.url | url }}">{{ item.title }}</a>{% endif %}</li>
{%- endfor %}
</ul>
</nav>
